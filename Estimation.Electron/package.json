{
  "name": "Estimation",
  "version": "0.0.1",
  "description": "Estimation program",
  "main": "app.js",
  "author": {
    "name": "Mattanapol.K"
  },
  "devDependencies": {
    "electron": "^2.0.5",
    "electron-builder": "^20.26.0",
    "electron-icon-maker": "0.0.4"
  },
  "dependencies": {
    "angular": "^1.6.1",
    "temp": "^0.8.3"
  },
  "scripts": {
    "start": "electron .",
    "dist": "build",
    "pack": "electron-builder --dir"
  },
  "build": {
    "appId": "Estimation",
    "directories": {
      "buildResources": "./assets",
      "output": "./dist"
    },
    "extraResources": {
      "from": "../Publish/win-x86/",
      "to": "server/",
      "filter": [
        "**/*"
      ]
    },
    "mac": {
      "category": "Estimation.App",
      "target": "dmg"
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "asar": false
  }
}
